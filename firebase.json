{
  "hosting": {
    "public": "dist/",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source" : "**/*.@(jpg|jpeg|gif|png|svg)",
      "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=345600"
      } ]
    } ]
  }
}
