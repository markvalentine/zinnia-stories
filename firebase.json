{
  "hosting": {
    "public": "dist/",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source" : "**/assets/**",
      "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=2592000"
      } ]
    },
    {
      "source" : "https://assets.pinterest.com/js/pinit.js",
      "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
      } ]
    },
    {
      "source" : "https://assets.pinterest.com/js/pinit_main.js?0.7257720790803432",
      "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
      } ]
    },
    {
      "source" : "https://w.soundcloud.com/player/api.js",
      "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
      } ]
    },
    {
      "source" : "https://connect.facebook.net/en_US/sdk.js",
      "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=604800"
      } ]
    },
    {
      "source" : "https://platform.twitter.com/widgets.js",
      "headers" : [ {
      "key" : "Cache-Control",
