{
  "hosting": {
    "public": "dist/",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source" : "**/assets/**",
      "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=2592000"
      } ]
    } ]
  }
}
